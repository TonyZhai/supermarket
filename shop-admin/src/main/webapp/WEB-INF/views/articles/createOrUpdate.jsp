<%@page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@include file="/WEB-INF/common/taglibs.jsp"%><div align="center">    <form id="form"/></div><script type="text/template" id="createTemplate">    <div>        <div>            <p>                <label>Title</label>                <input type="text" name="title" value="{{title}}"/>            </p>            <p>                <label>Content</label>                <input type="textarea" name="content" value="{{content}}" class="textarea"/>            </p>        </div>        <p>            <input type="button" id="btn_save" value="Save"/>            <input type="button" id="btn_cancel" value="Cancel"/>        </p>    </div></script><script type="text/javascript" src="${ctx}/static/javascript/views/articles/createOrUpdate.js"></script><script type="text/javascript">    $(function(){        Blog_View.context="${ctx}";        Blog_View.article.createOrUpdate.getInstance("${id}");    });</script>