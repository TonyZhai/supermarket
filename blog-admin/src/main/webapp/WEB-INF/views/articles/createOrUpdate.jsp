<%@page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@include file="/WEB-INF/common/taglibs.jsp"%><div class="bodyDiv" id="tabs">    <form:form id="form" method="POST" modelAttribute="article" action="${ctx}/articles">        <c:if test="${not empty article.id}">            <input type="hidden" name="_method" value="${requestMethod}"/>        </c:if>        <form:hidden path="id"/>        <form:hidden path="version"/>        <ul>            <li><a href="#tabs-1">Base Information</a></li>            <li><a href="#tabs-2">Attachment</a></li>        </ul>        <div id="tabs-1">            <p>                <label>Title</label>                <form:input path="title"/>            </p>            <p>                <label>Content</label>                <form:textarea path="content" cssClass="textarea"/>            </p>        </div>        <div id="tabs-2">            <input type="file"/>        </div>        <%--<p>--%>            <%--<label>Create Date</label>--%>            <%--<form:input path="baseInfo.createdDate" type="text" id="datepicker" />--%>        <%--</p>--%>        <p>            <input type="button" id="btn_save" value="Save"/>            <input type="button" id="btn_cancel" value="Cancel"/>        </p>    </form:form></div><script type="text/javascript" src="${ctx}/static/javascript/views/articles/article.js"></script><script type="text/javascript">    $(function(){        Blog_View.article.createOrUpdate.initPage("${ctx}");    });</script>